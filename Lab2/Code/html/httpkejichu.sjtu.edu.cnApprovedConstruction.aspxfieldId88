        $(function () {
            $(".govbox").children(":first").children(":first").children(":last").addClass("accordion-collapse").removeClass("accordion-expand");
            $(".govbox").children(":first").children(":last").show();
            $(".accordion_arrows").click(function () {
                var cls = $(this).attr("class");
                if (cls.indexOf("accordion-collapse") >= 0)//说明是展开的 需要收起来
                {
                    $(this).parent('.govbox_title').siblings('.govbox_list').slideUp("slow");
                    $(this).addClass("accordion-expand").removeClass("accordion-collapse");



                } else {
                    //说明是收缩的 需要展开
                    $(this).parent('.govbox_title').siblings('.govbox_list').slideDown("slow");
                    $(this).addClass("accordion-collapse").removeClass("accordion-expand");
                }
            })
        })
    </script>
